plotTree(vert.tree,offset=1,type="cladogram")
labelnodes(1:(Ntip(vert.tree)+vert.tree$Nnode),
1:(Ntip(vert.tree)+vert.tree$Nnode),
interactive=FALSE,cex=0.8)

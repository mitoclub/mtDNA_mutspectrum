#Chapter 1
text.string<-
"(((((Robin,Iguana),((((Cow,Whale),Pig),Bat),
(Lemur,Human))),Coelacanth),Goldfish),Shark);"
vert.tree<-read.tree(text=text.string)
plot(vert.tree,no.margin=TRUE)
